{
  "version": 2,
  "builds": [
    {
      "src": "bp_ib/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
  }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "bp_ib/wsgi.py"
    }
  ],
  "env": {
    "STATIC_ROOT": "/vercel/path0/public/staticfiles"
  }
}
